reen.clearScreen();\n  Brain.Screen.setCursor(1, 1);\n}\n\n// Generated code.\n\n// Include the IQ Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat myVariable;\n\n// \"when started\" hat block\nint whenStarted1() {\n  Drivetrain.setDriveVelocity(100.0, percent);\n  Drivetrain.setTurnVelocity(100.0, percent);\n  Lift.setVelocity(50.0, percent);\n  Lift.setStopping(hold);\n  Lift.setMaxTorque(100.0, percent);\n  Pickup.setMaxTorque(100.0, percent);\n  Pickup.setVelocity(100.0, percent);\n  Drivetrain.driveFor(forward, 200.0, mm, true);\n  Drivetrain.turnFor(left, 30.0, degrees, true);\n  Pickup.spin(forward);\n  Drivetrain.driveFor(forward, 300.0, mm, true);\n  return 0;\n}\n\n\nint main() {\n  // Calibrate the Drivetrain Gyro\n  calibrateDrivetrain();\n\n  whenStarted1();\n}","target":"Physical"}